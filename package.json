{
  "name": "@dark-bb/makka-pakka",
  "version": "1.0.1",
  "description": "为前端项目自动构建在pre-commit时，利用eslint+prettier统一代码风格",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/dark-bb/makka-pakka.git"
  },
  "author": {
    "name": "林逸舟",
    "email": "326943819@qq.com"
  },
  "license": "ISC",
  "keywords": [
    "eslint",
    "prettier",
    "lint-staged",
    "makka-pakka",
    "dark-bb"
  ],
  "files": [
    "bin",
    "assets"
  ],
  "scripts": {
    "testing": "node bin/testing.js",
    "postinstall": "node bin/index.js",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,json,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cross-spawn": "^7.0.3"
  }
}
